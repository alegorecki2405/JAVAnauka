	public static void main(String[] args) {
		Ocena ocenka1 = new Ocena("olek");
		Ocena ocenka2 = new Ocena("stszek");
		Field ocenka1imie = Ocena.class.getDeclaredField("imie");
		ocenka1imie.setAccessible(true);
		
		
				

	}

}
