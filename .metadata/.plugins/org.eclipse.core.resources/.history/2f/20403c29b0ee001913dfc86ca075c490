package naukajavy;

public class Trojkat {
	private int bok1;
	private int bok2;
	private int bok3;
	
	public void setter(int a , int b , int c) {
		this.bok1 = a;
		this.bok2 = b;
		this.bok3 = c;
	}
	public int geta() {
		return a;
	}
	public int getb() {
		return b;
	}
	public int getc() {
		return c;
	}
	
}
