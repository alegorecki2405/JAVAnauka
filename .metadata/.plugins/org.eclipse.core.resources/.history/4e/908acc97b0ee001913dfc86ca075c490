package naukajavy;

public class Trojkat {
	private int bok1;
	private int bok2;
	private int bok3;
	int p = (bok1+bok2+bok3)/2
	
	public void setter(int a , int b , int c) {
		this.bok1 = a;
		this.bok2 = b;
		this.bok3 = c;
	}
	public int geta() {
		return bok1;
	}
	public int getb() {
		return bok2;
	}
	public int getc() {
		return bok3;
	}
	public float pole()
	{
		float prawie = p(p-bok1)(p-bok2)(p-bok3);
	}
	
}
